{
  "name": "redux-boilerplate",
  "version": "1.0.0",
  "description": "a simple react, redux boilerplate with server-side rendering for heroku deployment",
  "main": "src/index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "test": "npm run test",
    "dev": "webpack-dev-server --inline --config webpack/dev.config.js",
    "prod": "npm run build && NODE_ENV=production node server/entry.js"
  },
  "keywords": [
    "react",
    "redux",
    "heroku",
    "webpack"
  ],
  "author": "jerekshoe",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.13.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.9.0",
    "bootstrap-loader": "^1.0.10",
    "bootstrap-sass": "^3.3.6",
    "css-loader": "^0.23.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "imports-loader": "^0.6.5",
    "jquery": "^3.1.0",
    "node-sass": "^3.10.0",
    "react": "^15.2.0",
    "react-cookie": "^0.4.7",
    "react-dom": "^15.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-promise": "^0.5.3",
    "redux-saga": "^0.11.0",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.0",
    "serialize-javascript": "^1.3.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-isomorphic-tools": "^2.4.2"
  },
  "devDependencies": {
    "babel-preset-react-hmre": "^1.1.1",
    "webpack-dev-server": "^1.14.1"
  }
}
